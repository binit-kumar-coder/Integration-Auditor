  "productSpecificRules": {
    "description": "Enhanced Shopify-NetSuite v2.0 specific validation rules",
    "netsuiteValidation": {
      "requireNetSuiteConnection": true,
      "requiredNetSuiteProperties": [
        "netsuiteConnectionId",
        "nsUtilImportAdaptorApiIdentifier",
        "nsUtilImportAdaptorId",
        "netsuiteAIConfig",
        "netsuiteMLModels",
        "netsuiteEnterpriseConfig"
      ],
      "validateNetSuiteSync": true,
      "requireAIFeatures": true,
      "requireEnterpriseFeatures": true,
      "validateMLModels": true
    },
    "shopifyValidation": {
      "requireShopifyConnection": true,
      "validateWebhooks": true,
      "requiredShopifyFeatures": [
        "inventory_sync",
        "order_sync",
        "customer_sync",
        "ai_recommendations",
        "real_time_analytics",
        "fraud_detection",
        "predictive_inventory",
        "enterprise_features"
      ],
      "requireShopifyPlusFeatures": true,
      "requireEnterpriseSubscription": true
    },
    "aiValidation": {
      "requireAIConfiguration": true,
      "validateMLModels": true,
      "requirePredictiveAnalytics": true,
      "validateAIEthicsCompliance": true,
      "requireEnterpriseAI": true
    },
    "enterpriseValidation": {
      "requireEnterpriseSupport": true,
      "validateGovernanceCompliance": true,
      "requireAdvancedSecurity": true,
      "validateDataGovernance": true
    }
  },
  
  "versionSpecificRules": {
    "description": "Shopify-NetSuite v2.0 enterprise features",
    "newFeatures": [
      "enterprise_ai_intelligence",
      "real_time_erp_synchronization",
      "advanced_financial_automation",
      "predictive_supply_chain_management",
      "ai_powered_business_optimization",
      "enterprise_compliance_automation"
    ],
    "breakingChanges": [
      "deprecated_v1_api_endpoints",
      "new_enterprise_authentication",
      "enhanced_ai_data_structures"
    ],
    "compatibilityRequirements": {
      "minNetSuiteVersion": "2024.2",
      "minShopifyAPIVersion": "2024-07",
      "requiresEnterpriseAILicense": true,
      "requiresAdvancedERPTier": true
    }
  },
  
  "licenseValidation": {
    "validEditions": ["starter", "premium", "enterprise"],
    "maxSettingsSize": 5242880,
    "productFamily": "shopify-netsuite",
    "requiresAILicense": true,
    "requiresEnterpriseSupport": true,
    "requiresAdvancedERPFeatures": true
  },
  
  "requiredProperties": {
    "topLevel": ["settings", "version"],
    "settingsLevel": ["connectorEdition", "commonresources", "aiConfig", "enterpriseConfig"],
    "commonresources": ["netsuiteConnectionId", "nsUtilImportAdaptorApiIdentifier", "nsUtilImportAdaptorId"],
    "aiConfig": ["mlModelVersion", "aiEthicsCompliance", "predictiveAnalyticsEnabled", "enterpriseAIEnabled"],
    "enterpriseConfig": ["governanceCompliance", "advancedSecurity", "dataGovernance", "complianceAutomation"]
  },
  
  "offlineConnectionRules": {
    "detectOfflineWithActiveResources": true,
    "crossReferenceFields": { "imports": "connectionId", "exports": "connectionId" },
    "connectionOfflineValues": [true, "true", "1"],
    "severity": "critical"
  },
  
  "updateProcessRules": {
    "updateInProgressField": "updateInProgress", 
    "updateInProgressValues": [true, "true", "1"],
    "csvFieldName": "UPDATEINPROGRESS",
    "severity": "critical"
  },
  
  "tolerances": {
    "resourceCountTolerance": 0,
    "aiModelAccuracyTolerance": 0.95,
    "enterpriseComplianceTolerance": 0
  },
  
  "metadata": {
    "businessOwner": "Enterprise ERP Team",
    "technicalOwner": "AI ERP Integration Team",
    "lastReviewed": "2025-09-23",
    "reviewCycle": "weekly",
    "changeApprovalRequired": true,
    "majorVersion": true,
    "enterpriseGrade": true,
    "aiEnabled": true
  }
}
